{
  "registry": "https://github.com/a4arpon/hot-shot/registry.json",
  "projectType": "ts",
  "contains": {
    "mods": [
      {
        "type": "router",
        "name": "products",
        "path": "./src/mods/products-mod",
        "contains": [
          "router",
          "controller",
          "services"
        ],
        "services": [
          "products.services",
          "reviews.services.js",
          "admin-products.services"
        ]
      },
      {
        "type": "router",
        "name": "cart",
        "path": "./src/mods/cart-mod",
        "contains": [
          "router",
          "controller",
          "services"
        ],
        "services": [
          "cart.services"
        ]
      },
      {
        "type": "router",
        "name": "auth-user",
        "path": "./src/mods/auth-user-mod",
        "contains": [
          "router",
          "controller",
          "services"
        ],
        "services": [
          "auth-user.services",
          "token.services.js",
          "verfication.services.js",
          "misc.services.js"
        ]
      }
    ]
  }
}
